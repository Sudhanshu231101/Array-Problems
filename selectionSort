#include<iostream>
using namespace std;

void bubbleSort(int a[], int n){
    for(int i=0;i<n;i++){
        int min=a[i];
        int minIndex=i;
        for(int j=i;j<n;j++){
            if(min>a[j]){
                min=a[j];
                minIndex=j;
            }
        }
        int temp=a[minIndex];
        a[minIndex]=a[i];
        a[i]=temp;
    }
    
}

int main(){
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    bubbleSort(a,n);
    
    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }
    return 0;
}
